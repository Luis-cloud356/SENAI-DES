generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model Carros {
  id     Int    @id @default(autoincrement())
  placa  String @unique
  marca  String
  modelo String
  ano    Int
}

model Clientes {
  id            Int    @id @default(autoincrement())
  nome_completo String
  cpf           Int
  email         String @unique
  cnh           String
}
